{
  "name": "calc-bound-move",
  "version": "1.0.0",
  "description": "Calculate the closest point from the current point to the next points, taking the looped structure into account",
  "main": "./dist/cjs/calcBoundMove.js",
  "module": "./dist/esm/calcBoundMove.js",
  "types": "./dist/type/calcBoundMove.d.ts",
  "exports": {
    ".": {
      "types": "./dist/type/calcBoundMove.d.ts",
      "require": "./dist/cjs/calcBoundMove.js",
      "import": "./dist/esm/calcBoundMove.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --module ESNext --outDir dist/esm",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types"
  },
  "keywords": [
    "util"
  ],
  "author": "kimgangmoon",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/web5-kimgangmoon/studyNpm.git"
  }
}
